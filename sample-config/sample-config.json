{
  "chatModel": {
    "type": "openai",
    "apiKeyEnv": "OPENAI_API_KEY",
    "model": "gpt-4.1-nano",
    "temperature": 0
  },
  "servers": [
    {
      "type": "stdio",
      "name": "server-memory",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    {
      "type": "stdio",
      "name": "filesystem",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/path"
      ]
    }
  ]
}